{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "verify-installation": "node src/scripts/verify-install.js && node src/scripts/copy-pspdfkit-files.js",
    "dev": "next dev",
    "build": "next build",
    "start": "npm run build && next start",
    "lint": "next lint",
    "codegen": "swagger-typescript-api --axios --patch  --union-enums -p http://localhost:5000/api/swagger.json -o ./src/__generated__ ",
    "postinstall": "npm run verify-installation"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "next": "12.0.7",
    "pspdfkit": "^2021.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swagger-typescript-api": "^9.3.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/react": "17.0.38",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ncp": "^2.0.0",
    "next-transpile-modules": "9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.3"
  }
}
